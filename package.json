		"protractor": "~5.1.2",
		"ts-node": "~4.1.0",
		"tslint": "~5.9.1",
		"typescript": "3.1.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,json,scss,md}": [
			"prettier --write",
			"git add"
